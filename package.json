{
  "name": "silvertrade",
  "version": "1.0.0",
  "description": "Node.js react MongoDb coin shop",
  "main": "server.js",
  "engines": {
    "node": "10.23.3"
  },
  "scripts": {
    "build": "cd client && npm run build",
    "start": "nodemon server.js",
    "test": "set NODE_ENV=production && mocha --timeout 10000 \"./{,!(node_modules|client)/**/}*.test.js\"",
    "test:watch": "onchange \"./**/*.js\" -i -k -- npm test"
  },
  "keywords": [
    "node",
    "express",
    "api"
  ],
  "author": "stiffelbo",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "mocha": "^6.2.1",
    "mongodb": "^3.3.2",
    "mongodb-memory-server": "^6.9.6",
    "mongoose": "^5.7.1",
    "nock": "^13.1.0",
    "nodemon": "^1.19.1",
    "onchange": "^6.1.0",
    "request": "^2.88.2",
    "socket.io": "^2.2.0"
  }
}
